{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "required": [
        "products"
    ],
    "properties": {
        "products": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "title",
                    "description",
                    "price",
                    "discountPercentage",
                    "rating",
                    "stock",
                    "brand",
                    "category",
                    "thumbnail",
                    "images"
                ],
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "title": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "price": {
                        "type": "number"
                    },
                    "discountPercentage": {
                        "type": "number"
                    },
                    "rating": {
                        "type": "number"
                    },
                    "stock": {
                        "type": "number"
                    },
                    "brand": {
                        "type": "string"
                    },
                    "category": {
                        "type": "string"
                    },
                    "thumbnail": {
                        "type": "string"
                    },
                    "images": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "total": {
            "type": "number"
        },
        "skip": {
            "type": "number"
        },
        "limit": {
            "type": "number"
        }
    }
}